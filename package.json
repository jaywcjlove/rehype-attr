{
  "name": "rehype-attr",
  "version": "2.0.3",
  "description": "New syntax to add attributes to Markdown.",
  "author": "Kenny Wong <wowohoo@qq.com>",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "lib/index.js",
  "scripts": {
    "prepack": "npm run build",
    "start": "node lib/index.js",
    "watch": "tsbb watch --disable-babel",
    "build": "tsbb build --disable-babel && npm run esm-fix",
    "esm-fix": "tsc-esm-fix --target='lib' --ext='.js'",
    "type-check": "tsc --noEmit",
    "test": "tsbb test",
    "coverage": "tsbb test --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/rehype-attr"
  },
  "files": [
    "src",
    "lib"
  ],
  "keywords": [
    "attributes",
    "rehype",
    "markdown",
    "rehype-attr",
    "javascript",
    "html",
    "ast",
    "unified"
  ],
  "jest": {
    "testMatch": [
      "<rootDir>/test/*.{ts,tsx}"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/*.{tsx,ts}"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/?!(.*)"
    ]
  },
  "dependencies": {
    "unified": "10.1.0"
  },
  "devDependencies": {
    "@types/jest": "27.0.1",
    "rehype": "12.0.0",
    "rehype-raw": "6.1.0",
    "rehype-stringify": "9.0.2",
    "remark-gfm": "2.0.0",
    "remark-parse": "10.0.0",
    "remark-rehype": "9.0.0",
    "tsc-esm-fix": "2.7.2",
    "tsbb": "3.2.1"
  }
}
